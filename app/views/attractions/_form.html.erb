
<%= form_for(@attraction) do |f| %> 
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :tickets %>
  <%= f.text_field :tickets %><br>
  <%= f.label :nausea_rating, "Nausea Rating (0-10)" %>
  <%= f.text_field :nausea_rating %><br>
  <%= f.label :happiness_rating, "Happiness Rating (0-10)" %>
  <%= f.text_field :happiness_rating %><br>
  <%= f.label :min_height, "Minimum Height (inches" %>
  <%= f.text_field :min_height %><br>
  <%= f.submit "#{@action} Attraction"%>
<% end %>